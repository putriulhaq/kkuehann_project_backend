{
  "version": 2,
  "builds": [
    {
      "src": "vercel_build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "." }
    },
    {
      "src": "app/__init__.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/__init__.py"
    }
  ]
}
